{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not edge <= 114",
    "not ios_saf <= 16.5",
    "not safari <= 16.5",
    "not and_ff <= 113"
  ]
}